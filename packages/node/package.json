{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node",
  "version": "3.6.3",
  "description": "XYO Express Node CLI",
  "license": "LGPL-3.0-only",
  "author": "Arie Trouw",
  "type": "module",
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/browser/index.mjs"
      },
      "node": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/node/index.mjs"
      },
      "types": "./dist/types/index.d.ts",
      "default": "./dist/node/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "module": "dist/node/index.mjs",
  "types": "dist/types/index.d.ts",
  "workspaces": [
    "packages/**/*"
  ],
  "dependencies": {
    "@xyo-network/express-node-dependencies": "workspace:^",
    "@xyo-network/express-node-lib": "workspace:^",
    "@xyo-network/express-node-routes": "workspace:^",
    "@xyo-network/express-node-server": "workspace:^"
  },
  "devDependencies": {
    "@swc/core": "^1.11.24",
    "@xylabs/ts-scripts-yarn3": "^6.4.6",
    "@xylabs/tsconfig": "^6.4.6",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideeffects": false
}
