{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/node-core-lib",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.82.0",
  "description": "XYO Archivist",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "require": {
          "types": "./dist/browser/index.d.cts",
          "default": "./dist/browser/index.cjs"
        },
        "import": {
          "types": "./dist/browser/index.d.mts",
          "default": "./dist/browser/index.js"
        }
      },
      "node": {
        "require": {
          "types": "./dist/node/index.d.ts",
          "default": "./dist/node/index.js"
        },
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.mjs"
        }
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.js",
  "module": "dist/node/index.mjs",
  "author": "Arie Trouw",
  "license": "LGPL-3.0-only",
  "dependencies": {
    "@xyo-network/boundwitness-model": "^2.83.2",
    "@xyo-network/node-core-model": "^2.82.0",
    "@xyo-network/payload-model": "^2.83.2",
    "@xyo-network/payload-mongodb": "workspace:~",
    "@xyo-network/payload-wrapper": "^2.83.2",
    "@xyo-network/schema-cache": "^2.83.2",
    "@xyo-network/schema-payload-plugin": "^2.83.0",
    "ajv": "^8.12.0"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^3.2.10",
    "@xylabs/tsconfig": "^3.2.10",
    "@xyo-network/boundwitness-builder": "^2.83.2",
    "@xyo-network/core": "^2.83.2",
    "@xyo-network/payload-builder": "^2.83.2",
    "typescript": "^5.3.3"
  }
}
