  "dependencies": {
    "@xylabs/express": "^5.0.0",
    "@xyo-network/account-model": "^5.0.0",
    "@xyo-network/express-node-server": "workspace:^",
    "@xyo-network/node-core-types": "workspace:^",
    "@xyo-network/node-memory": "^5.0.0",
    "@xyo-network/node-model": "^5.0.0",
    "@xyo-network/payload-validator": "^5.0.0",
    "@xyo-network/schema-name-validator": "^5.0.0",
    "@xyo-network/wallet": "^5.0.0",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^7.0.2",
    "@xylabs/tsconfig": "^7.0.2",
    "@xylabs/vitest-extended": "^5.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideeffects": false
}
