{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/node-app",
  "version": "4.1.5",
  "description": "XYO Node App",
  "license": "LGPL-3.0-only",
  "author": "Arie Trouw",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/node/index.d.ts",
      "default": "./dist/node/index.mjs"
    },
    "./app": {
      "types": "./dist/node/app.d.ts",
      "default": "./dist/node/app.mjs"
    },
    "./package.json": "./package.json"
  },
  "module": "dist/node/index.mjs",
  "types": "dist/node/index.d.ts",
  "files": [
    "dist",
    "src",
    "!**/*.spec.*",
    "!**/*.test.*"
  ],
  "dependencies": {
    "@xyo-network/account-model": "~5.2.16",
    "@xyo-network/express-node-server": "workspace:^",
    "@xyo-network/node-core-types": "workspace:^",
    "@xyo-network/node-memory": "~5.2.16",
    "@xyo-network/node-model": "~5.2.16",
    "@xyo-network/payload-validator": "~5.2.16",
    "@xyo-network/schema-name-validator": "~5.2.16",
    "@xyo-network/wallet": "~5.2.16",
    "dotenv": "~17.2.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "~7.2.8",
    "@xylabs/tsconfig": "~7.2.8",
    "@xylabs/vitest-extended": "~5.0.50",
    "axios": "~1.13.2",
    "typescript": "~5.9.3",
    "vitest": "~4.0.15"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideeffects": false
}
