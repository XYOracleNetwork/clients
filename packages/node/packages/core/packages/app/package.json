{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/node-app",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.89.1",
  "description": "XYO Node App",
  "sideeffects": false,
  "exports": {
    ".": {
      "node": {
        "require": {
          "types": "./dist/node/index.d.cts",
          "default": "./dist/node/index.cjs"
        },
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.mjs"
        }
      }
    },
    "./app": {
      "node": {
        "require": {
          "types": "./dist/node/app.d.cts",
          "default": "./dist/node/app.cjs"
        },
        "import": {
          "types": "./dist/node/app.d.mts",
          "default": "./dist/node/app.mjs"
        }
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.cjs",
  "module": "dist/node/index.mjs",
  "types": "dist/node/index.d.ts",
  "dependencies": {
    "@xylabs/sdk-api-express-ecs": "^2.0.4",
    "@xyo-network/account": "^2.111.2",
    "@xyo-network/account-model": "^2.111.2",
    "@xyo-network/express-node-server": "workspace:^",
    "@xyo-network/node-core-types": "workspace:^",
    "@xyo-network/node-memory": "^2.111.2",
    "@xyo-network/node-model": "^2.111.2",
    "@xyo-network/payload-validator": "^2.111.2",
    "@xyo-network/schema-name-validator": "^2.111.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^3.15.14",
    "@xylabs/tsconfig": "^3.15.14",
    "typescript": "^5.5.4"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0-only",
  "type": "module"
}