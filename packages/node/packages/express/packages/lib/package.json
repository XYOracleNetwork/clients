{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-lib",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.88.7",
  "description": "XYO Express Node Lib",
  "sideeffects": false,
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "require": {
          "types": "./dist/browser/index.d.cts",
          "default": "./dist/browser/index.cjs"
        },
        "import": {
          "types": "./dist/browser/index.d.mts",
          "default": "./dist/browser/index.js"
        }
      },
      "node": {
        "require": {
          "types": "./dist/node/index.d.cts",
          "default": "./dist/node/index.cjs"
        },
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.js"
        }
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.cjs",
  "module": "dist/node/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0-only",
  "dependencies": {
    "@xylabs/assert": "^3.5.1",
    "@xylabs/sdk-api-express-ecs": "^2.0.4",
    "@xyo-network/module-model": "^2.106.0",
    "@xyo-network/node-model": "^2.106.0",
    "@xyo-network/payload-mongodb": "workspace:~",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@xylabs/ts-scripts-yarn3": "^3.11.2",
    "@xylabs/tsconfig": "^3.11.2",
    "typescript": "^5.4.5"
  },
  "type": "module"
}
