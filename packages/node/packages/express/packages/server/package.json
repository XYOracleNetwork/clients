{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-server",
  "version": "4.1.6",
  "description": "XYO Express Node Server",
  "license": "LGPL-3.0-only",
  "author": "Arie Trouw",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/browser/index.d.ts",
        "default": "./dist/browser/index.mjs"
      },
      "node": {
        "types": "./dist/node/index.d.ts",
        "default": "./dist/node/index.mjs"
      },
      "types": "./dist/node/index.d.ts",
      "default": "./dist/node/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "module": "dist/node/index.mjs",
  "types": "dist/node/index.d.ts",
  "files": [
    "dist",
    "src",
    "!**/*.spec.*",
    "!**/*.test.*"
  ],
  "dependencies": {
    "@xylabs/assert": "~5.0.61",
    "@xylabs/express": "~5.0.61",
    "@xyo-network/express-node-dependencies": "workspace:^",
    "@xyo-network/express-node-routes": "workspace:^",
    "@xyo-network/node-core-types": "workspace:^",
    "@xyo-network/node-memory": "~5.2.22",
    "@xyo-network/node-model": "~5.2.22",
    "compression": "~1.8.1",
    "cors": "~2.8.5",
    "express": "~5.2.1"
  },
  "devDependencies": {
    "@types/compression": "~1.8.1",
    "@types/cors": "~2.8.19",
    "@types/express": "~5.0.6",
    "@types/nconf": "~0.10.7",
    "@xylabs/logger": "~5.0.61",
    "@xylabs/ts-scripts-yarn3": "~7.2.32",
    "@xylabs/tsconfig": "~7.2.32",
    "@xyo-network/node-core-model": "workspace:^",
    "@xyo-network/shared": "~5.2.22",
    "nconf": "~0.13.0",
    "typescript": "~5.9.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideeffects": false
}
