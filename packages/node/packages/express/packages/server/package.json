{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-server",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.88.9",
  "description": "XYO Express Node Server",
  "sideeffects": false,
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "require": {
          "types": "./dist/browser/index.d.cts",
          "default": "./dist/browser/index.cjs"
        },
        "import": {
          "types": "./dist/browser/index.d.mts",
          "default": "./dist/browser/index.js"
        }
      },
      "node": {
        "require": {
          "types": "./dist/node/index.d.cts",
          "default": "./dist/node/index.cjs"
        },
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.js"
        }
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.cjs",
  "module": "dist/node/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0-only",
  "dependencies": {
    "@xylabs/assert": "^3.5.2",
    "@xylabs/logger": "^3.5.2",
    "@xylabs/sdk-api-express-ecs": "^2.0.4",
    "@xyo-network/express-node-dependencies": "workspace:^",
    "@xyo-network/express-node-middleware": "workspace:^",
    "@xyo-network/express-node-routes": "workspace:^",
    "@xyo-network/node-core-model": "workspace:^",
    "@xyo-network/node-core-types": "workspace:^",
    "@xyo-network/node-memory": "^2.107.6",
    "@xyo-network/node-model": "^2.107.6",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "nconf": "^0.12.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/nconf": "^0.10.6",
    "@xylabs/ts-scripts-yarn3": "^3.11.12",
    "@xylabs/tsconfig": "^3.11.12",
    "typescript": "^5.5.3"
  },
  "type": "module"
}
