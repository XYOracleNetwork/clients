{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-server",
  "version": "2.89.2",
  "description": "XYO Express Node Server",
  "license": "LGPL-3.0-only",
  "author": "Arie Trouw",
  "type": "module",
  "exports": {
    ".": {
      "browser": {
        "import": {
          "types": "./dist/browser/index.d.ts",
          "default": "./dist/browser/index.mjs"
        }
      },
      "node": {
        "import": {
          "types": "./dist/node/index.d.ts",
          "default": "./dist/node/index.mjs"
        }
      }
    },
    "./package.json": "./package.json"
  },
  "module": "dist/node/index.mjs",
  "types": "dist/node/index.d.ts",
  "dependencies": {
    "@xylabs/assert": "^4.0.5",
    "@xylabs/logger": "^4.0.5",
    "@xylabs/sdk-api-express-ecs": "^2.0.4",
    "@xyo-network/express-node-dependencies": "workspace:^",
    "@xyo-network/express-node-middleware": "workspace:^",
    "@xyo-network/express-node-routes": "workspace:^",
    "@xyo-network/node-core-model": "workspace:^",
    "@xyo-network/node-core-types": "workspace:^",
    "@xyo-network/node-memory": "^3.0.11",
    "@xyo-network/node-model": "^3.0.11",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "nconf": "^0.12.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
    "@types/nconf": "^0.10.7",
    "@xylabs/ts-scripts-yarn3": "^4.0.7",
    "@xylabs/tsconfig": "^4.0.7",
    "typescript": "^5.5.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideeffects": false
}
