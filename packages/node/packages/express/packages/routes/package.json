{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/express-node-routes",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.88.8",
  "description": "XYO Express Node Routes",
  "sideeffects": false,
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "require": {
          "types": "./dist/browser/index.d.cts",
          "default": "./dist/browser/index.cjs"
        },
        "import": {
          "types": "./dist/browser/index.d.mts",
          "default": "./dist/browser/index.js"
        }
      },
      "node": {
        "require": {
          "types": "./dist/node/index.d.cts",
          "default": "./dist/node/index.cjs"
        },
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.js"
        }
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.cjs",
  "module": "dist/node/index.js",
  "author": "Arie Trouw",
  "license": "LGPL-3.0-only",
  "dependencies": {
    "@xylabs/assert": "^3.5.2",
    "@xylabs/exists": "^3.5.2",
    "@xylabs/hex": "^3.5.2",
    "@xylabs/sdk-api-express-ecs": "^2.0.4",
    "@xyo-network/archivist": "^2.107.5",
    "@xyo-network/archivist-model": "^2.107.5",
    "@xyo-network/boundwitness-model": "^2.107.5",
    "@xyo-network/boundwitness-wrapper": "^2.107.5",
    "@xyo-network/diviner-boundwitness-abstract": "^2.107.5",
    "@xyo-network/diviner-boundwitness-model": "^2.107.5",
    "@xyo-network/diviner-model": "^2.107.5",
    "@xyo-network/diviner-payload-abstract": "^2.107.5",
    "@xyo-network/diviner-payload-model": "^2.107.5",
    "@xyo-network/domain-payload-plugin": "^2.107.5",
    "@xyo-network/express-node-lib": "workspace:~",
    "@xyo-network/express-node-middleware": "workspace:~",
    "@xyo-network/module-abstract": "^2.107.5",
    "@xyo-network/module-model": "^2.107.5",
    "@xyo-network/node-core-lib": "workspace:~",
    "@xyo-network/node-core-model": "workspace:~",
    "@xyo-network/node-core-types": "workspace:~",
    "@xyo-network/payload-model": "^2.107.5",
    "@xyo-network/payload-wrapper": "^2.107.5",
    "@xyo-network/prometheus-node-plugin": "^2.97.1",
    "express": "^4.19.2",
    "http-status-codes": "^2.3.0"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@xylabs/ts-scripts-yarn3": "^3.11.11",
    "@xylabs/tsconfig": "^3.11.11",
    "@xyo-network/account": "^2.107.5",
    "@xyo-network/account-model": "^2.107.5",
    "@xyo-network/boundwitness-builder": "^2.107.5",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "typescript": "^5.5.2"
  },
  "type": "module"
}
