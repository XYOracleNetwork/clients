{
  "name": "@xyo-network/sdk-xyo-mongo-js",
  "version": "2.82.0",
  "homepage": "https://xyo.network",
  "bugs": {
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js/issues",
    "email": "support@xyo.network"
  },
  "description": "Base functionality used throughout XYO TypeScript/JavaScript libraries that access Mongo DB",
  "author": {
    "name": "XYO Development Team",
    "url": "https://xyo.network",
    "email": "support@xyo.network"
  },
  "license": "LGPL-3.0-only",
  "repository": {
    "type": "git",
    "url": "https://github.com/XYOracleNetwork/sdk-xyo-mongo-js.git"
  },
  "types": "dist/node/index.d.ts",
  "exports": {
    ".": {
      "browser": {
        "require": {
          "types": "./dist/browser/index.d.cts",
          "default": "./dist/browser/index.cjs"
        },
        "import": {
          "types": "./dist/browser/index.d.mts",
          "default": "./dist/browser/index.js"
        }
      },
      "node": {
        "require": {
          "types": "./dist/node/index.d.cts",
          "default": "./dist/node/index.cjs"
        },
        "import": {
          "types": "./dist/node/index.d.mts",
          "default": "./dist/node/index.js"
        }
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/node/index.cjs",
  "module": "dist/node/index.js",
  "keywords": [
    "xyo",
    "utility",
    "typescript",
    "mongo"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@xylabs/assert": "^2.13.20",
    "@xylabs/delay": "^2.13.20",
    "@xylabs/forget": "^2.13.20",
    "async-mutex": "^0.4.0",
    "mongodb": "^6.3.0"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^3.2.17",
    "@xylabs/tsconfig": "^3.2.17",
    "typescript": "^5.3.3"
  },
  "type": "module"
}
