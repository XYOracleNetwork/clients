    "@xyo-network/blocknative-ethereum-gas-plugin": "next",
    "@xyo-network/coingecko-crypto-market-plugin": "next",
    "@xyo-network/crypto-asset-payload-plugin": "next",
    "@xyo-network/crypto-asset-plugin": "next",
    "@xyo-network/diviner-model": "next",
    "@xyo-network/etherchain-ethereum-gas-v2-plugin": "next",
    "@xyo-network/ethers-ethereum-gas-plugin": "next",
    "@xyo-network/etherscan-ethereum-gas-plugin": "next",
    "@xyo-network/gas-price-payload-plugin": "next",
    "@xyo-network/gas-price-plugin": "next",
    "@xyo-network/http-bridge": "next",
    "@xyo-network/module-model": "next",
    "@xyo-network/node-core-model": "^2.83.0",
    "@xyo-network/node-memory": "next",
    "@xyo-network/payload-model": "next",
    "@xyo-network/sentinel": "next",
    "@xyo-network/shared": "next",
