    "@xyo-network/uniswap-crypto-market-plugin": "next",
    "@xyo-network/witness-adhoc": "next",
    "@xyo-network/witness-model": "next",
    "agenda": "^5.0.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "ethers": "^6.10.0",
    "express": "^4.18.2",
    "express-mung": "^0.5.1",
    "http-status-codes": "^2.3.0",
    "node-cache": "^5.1.2",
    "rollbar": "^2.26.2",
    "winston": "^3.11.0",
    "winston-transport": "^4.6.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "4.17.21",
