{
  "$schema": "https://raw.githubusercontent.com/XYOracleNetwork/sdk-xyo-client-js/a5a229a767e3998e13cce8160fd1c2b027317424/packages/manifest/src/schema.json",
  "nodes": [
    {
      "config": {
        "accountPath": "1",
        "name": "XyoMediumRssFeed",
        "schema": "network.xyo.node.config"
      },
      "modules": {
        "private": [
          {
            "config": {
              "accountPath": "1/1'/1'",
              "name": "ApiCallWitness",
              "schema": "network.xyo.api.call.witness.config",
              "uriTemplate": "https://medium.com/feed/${feed}",
              "accept": "text/xml"
            }
          },
          {
            "config": {
              "accountPath": "1/1'/2'",
              "name": "XmlDiviner",
              "schema": "network.xyo.diviner.xml.config"
            }
          },
          {
            "config": {
              "accountPath": "1/1'/3'",
              "name": "JsonPathDiviner",
              "schema": "network.xyo.diviner.jsonpath.config",
              "transforms": [
                {
                  "destinationField": "item",
                  "sourcePathExpression": "$.xml.rss.channel[0].item"
                }
              ]
            }
          }
        ],
        "public": [
          {
            "config": {
              "accountPath": "1/1/1",
              "name": "ApiCallSentinel",
              "schema": "network.xyo.sentinel.config",
              "synchronous": "true",
              "tasks": [
                {
                  "input": true,
                  "module": "ApiCallWitness"
                },
                {
                  "input": ["ApiCallWitness"],
                  "module": "XmlDiviner"
                },
                {
                  "input": ["XmlDiviner"],
                  "module": "JsonPathDiviner"
                }
              ]
            }
          }
        ]
      }
    }
  ],
  "schema": "network.xyo.manifest"
}
